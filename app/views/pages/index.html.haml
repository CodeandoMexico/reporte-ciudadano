.row.col-md-6.col-md-offset-3.padding-video
  =embed_video(@url_video)
.row.col-md-6.col-md-offset-3
  =t('.video_explication')
.row.reports-stats{data: { total: ServiceRequest.count } }
  = render "kpis/kpi", kpi: pretty_kpi_data(@all_service_requests,t('dashboards.total_cities'))
  = render "kpis/kpi", kpi: pretty_kpi_data(@open_service_requests,t('dashboards.total_procedures_services'))
  = render "kpis/kpi", kpi: pretty_kpi_data(@closed_service_requests,t('dashboards.total_evaluations'))

.h1.row.col-md-6.col-md-offset-3.text-center
  =t(".participa")
.col-md-12.text-center  
  .col-md-6.padding-top
    =link_to t(".queja_sugerencia"), service_requests_path, :class=> 'reports new btn btn-primary navbar-btn half_content'
  .col-md-6.padding-top
    =link_to t(".evalua_tramite"), service_surveys_path, :class=> 'reports new btn btn-primary navbar-btn half_content'