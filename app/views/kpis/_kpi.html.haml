.col-lg-4.col-md-6.padding-top
  = content_tag :div, class: "panel #{kpi[:panel]} #{kpi[:name]}" do
    .panel-heading
      .row
        .col-xs-12
          = content_tag :div, kpi[:value], class: 'text-center number-kpi'
        .col-xs-12.padding-top-kpi
          = content_tag :div, kpi[:message], class: 'text-center text-kpi'
              